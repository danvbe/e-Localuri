{% extends 'LocaluriLogBundle:Log:layout.html.twig' %}

{% block body %}
<h1>Log list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Operation</th>
            <th>Ip</th>
            <th>User</th>
            <th>Entity name</th>
            <th>Description</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('lcl_log_admin_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.operation }}</td>
            <td>{{ entity.ip }}</td>
            <td>{{ entity.user }}</td>
            <td>{{ entity.entityName }}</td>
            <td>{{ entity.description }}</td>
            <td><a href="{{ path('lcl_log_admin_show', { 'id': entity.id }) }}">show</a></td>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock %}